{
    "name": "ionic-dominicana",
    "version": "1.0.0",
    "author": "@1antares1",
    "homepage": "https://ionic-dominican.com/",
    "scripts": {
        "ng": "ng",
        "start": "ionic serve",
        "build:web": "ng build",
        "build:ionic": "ng build && npm run cap.copy",
        "build:mobile": "npm run cap.copy && npm run cap.update",
        "build:mobile:ios": "npm run cap.copy && npx cap update ios",
        "build:mobile:android": "npm run cap.copy && npx cap update android",
        "build:prod": "npm run build:web -- --prod && npm run build:mobile",
        "cap.add.ios": "npx cap add ios",
        "cap.add.android": "npx cap add android",
        "cap.ios": "npx cap open ios",
        "cap.android": "npx cap open android",
        "cap.copy": "npx cap copy",
        "cap.update": "npx cap update",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start:ssl": "ng serve --ssl 1 --ssl-key \"src/server/certs/ionicdominican.com.do.key.pem\" --ssl-cert \"src/server/certs/ionicdominican.com.do.key.pem/ionicdominican.com.do.cert.pem\"",
        "start:dev": "ng serve --configuration dev",
        "start:prod": "ng serve --configuration prod",
        "start:hmr": "ng serve --configuration hmr --aot=true",
        "start:hmr:ssl": "ng serve --configuration hmr --ssl 1 --ssl-key \"src/server/certs/ionicdominican.com.do.key.pem\" --ssl-cert \"src/server/certs/ionicdominican.com.do.key.pem/ionicdominican.com.do.cert.pem\"",
        "build:i18n": "ng xi18n --output-path src/i18n --outFile resources.en.xlf",
        "build:full": "npm run clean:dist && npm run build:server && npm run build",
        "build:server": "tsc --project src/server/tsconfig.server.json --sourceMap",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "cls": "clear && printf '\\e[3J'",
        "debug": "node --debug-brk app.js",
        "install:prod": "npm update && npm install --production && node node_modules/node-sass/scripts/install.js && npm rebuild node-sass",
        "docker": "docker",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "docs:compodoc": "compodoc -p tsconfig.json",
        "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
        "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
        "node": "node",
        "node:killprocesses": "killall node && sudo kill $(sudo lsof -t -i:4200)",
        "postversion": "git push && git push --tags",
        "post:heroku:release": "git push && git push --tags && git push heroku-prod master",
        "rimraf": "rimraf",
        "server:prod": "set NODE_ENV=production && node server/app.js",
        "server:deploy": "git push heroku-prod master:master",
        "server-start": "node server/app.js",
        "tslint": "tslint",
        "analyzer": "webpack-bundle-analyzer dist/stats.json"
    },
    "dependencies": {
        "@angular/animations": "file:../../../node_modules/@angular/animations",
        "@angular/common": "file:../../../node_modules/@angular/common",
        "@angular/core": "file:../../../node_modules/@angular/core",
        "@angular/fire": "file../../../node_modules/@angular/fire",
        "@angular/forms": "file:../../../node_modules/@angular/forms",
        "@angular/platform-browser": "file:../../../node_modules/@angular/platform-browser",
        "@angular/platform-browser-dynamic": "file:../../../node_modules/@angular/platform-browser-dynamic",
        "@angular/service-worker": "file:../../../node_modules/@angular/service-worker",
        "@angular/router": "file:../../../node_modules/@angular/router",
        "@capacitor/android": "2.0.1",
        "@capacitor/core": "2.0.1",
        "@capacitor/ios": "2.0.1",
        "@ionic-native/core": "file:../../../node_modules/@ionic-native/core",
        "@ionic-native/splash-screen": "file:../../../node_modules/@ionic-native/splash-screen",
        "@ionic-native/status-bar": "file:../../../node_modules/@ionic-native/status-bar",
        "@ionic/angular": "file:../../../node_modules/@ionic/angular",
        "@ngx-translate/core": "file:../../../node_modules/@ngx-translate/core",
        "@ngx-translate/http-loader": "file:../../../node_modules/@ngx-translate/http-loader",
        "@workspace/ionic-scss": "file:../../../xplat/ionic/scss",
        "@workspace/scss": "file:../../../libs/scss",
        "rxjs": "file:../../../node_modules/rxjs",
        "zone.js": "file:../../../node_modules/zone.js"
    },
    "devDependencies": {
        "@capacitor/cli": "1.1.1",
        "@angular-devkit/architect": "file:../../../node_modules/@angular-devkit/architect",
        "@angular-devkit/build-angular": "file:../../../node_modules/@angular-devkit/build-angular",
        "@angular-devkit/core": "file:../../../node_modules/@angular-devkit/core",
        "@angular-devkit/schematics": "file:../../../node_modules/@angular-devkit/schematics",
        "@angular/cli": "file:../../../node_modules/@angular/cli",
        "@angular/compiler": "file:../../../node_modules/@angular/compiler",
        "@angular/compiler-cli": "file:../../../node_modules/@angular/compiler-cli",
        "@angular/language-service": "file:../../../node_modules/@angular/language-service",
        "@ionic/angular-toolkit": "file:../../../node_modules/@ionic/angular-toolkit",
        "@types/jasmine": "file:../../../node_modules/@types/jasmine",
        "@types/jasminewd2": "file:../../../node_modules/@types/jasminewd2",
        "@types/node": "file:../../../node_modules/@types/node",
        "codelyzer": "file:../../../node_modules/codelyzer",
        "jasmine-core": "file:../../../node_modules/jasmine-core",
        "jasmine-spec-reporter": "file:../../../node_modules/jasmine-spec-reporter",
        "karma": "file:../../../node_modules/karma",
        "karma-chrome-launcher": "file:../../../node_modules/karma-chrome-launcher",
        "karma-coverage-istanbul-reporter": "file:../../../node_modules/karma-coverage-istanbul-reporter",
        "karma-jasmine": "file:../../../node_modules/karma-jasmine",
        "karma-jasmine-html-reporter": "file:../../../node_modules/karma-jasmine-html-reporter",
        "protractor": "file:../../../node_modules/protractor",
        "ts-node": "file:../../../node_modules/ts-node",
        "tslint": "file:../../../node_modules/tslint",
        "typescript": "file:../../../node_modules/typescript"
    }
}
